  let (n, str) := tuple_0
  if (BEq.beq (String.take str 1) "-")
  then (valid_hex_bits n (String.drop str 1))
  else
    if (valid_hex_bits n str)
    then
      let parsed := (parse_hex_bits n str)
      (BEq.beq (BitVec.access parsed (n -i 1)) 0#1)
    else false

